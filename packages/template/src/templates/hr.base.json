{
  "Human Resource Management": {
    "tables": {
      "Employees": {
        "fieldsOrder": [
          "Name",
          "Position",
          "Department",
          "Department Name",
          "Manage Department",
          "Manage Department Name",
          "Start Date",
          "Salary",
          "Reports To",
          "Manager Name",
          "Subordinates",
          "Subordinates Count",
          "Subordinates Name",
          "Email",
          "Phone",
          "Employment Status"
        ],
        "schema": {
          "Name": {
            "id": "name",
            "type": "string",
            "constraint": {
              "required": true
            },
            "display": true
          },
          "Position": {
            "id": "position",
            "type": "select",
            "constraint": {
              "max": 1,
              "required": true
            },
            "option": {
              "options": [
                {
                  "id": "developer",
                  "name": "Developer",
                  "color": "blue"
                },
                {
                  "id": "manager",
                  "name": "Manager",
                  "color": "green"
                },
                {
                  "id": "hr",
                  "name": "HR Specialist",
                  "color": "purple"
                },
                {
                  "id": "designer",
                  "name": "Designer",
                  "color": "yellow"
                },
                {
                  "id": "marketing",
                  "name": "Marketing Specialist",
                  "color": "orange"
                },
                {
                  "id": "finance",
                  "name": "Finance Analyst",
                  "color": "red"
                }
              ]
            }
          },
          "Department": {
            "id": "department",
            "type": "reference",
            "option": {
              "createSymmetricField": true,
              "foreignTable": {
                "tableName": "Departments"
              }
            }
          },
          "Department Name": {
            "id": "department_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "department",
              "rollupFieldId": "department_name",
              "fn": "lookup"
            }
          },
          "Manage Department": {
            "id": "manage_department",
            "type": "reference",
            "option": {
              "symmetricFieldId": "manager"
            }
          },
          "Manage Department Name": {
            "id": "manage_department_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "manage_department",
              "rollupFieldId": "department_name",
              "fn": "lookup"
            }
          },
          "Start Date": {
            "id": "start_date",
            "type": "date",
            "constraint": {
              "required": true
            }
          },
          "Salary": {
            "id": "salary",
            "type": "currency",
            "option": {
              "symbol": "$"
            }
          },
          "Reports To": {
            "id": "reports_to",
            "type": "reference",
            "option": {
              "createSymmetricField": true,
              "foreignTable": {
                "tableName": "Employees"
              }
            }
          },
          "Subordinates": {
            "id": "subordinates",
            "type": "reference",
            "option": {
              "symmetricFieldId": "reports_to"
            }
          },
          "Manager Name": {
            "id": "manager_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "reports_to",
              "rollupFieldId": "name",
              "fn": "lookup"
            }
          },
          "Subordinates Count": {
            "id": "subordinates_count",
            "type": "rollup",
            "option": {
              "referenceFieldId": "subordinates",
              "rollupFieldId": "id",
              "fn": "count"
            }
          },
          "Subordinates Name": {
            "id": "subordinates_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "subordinates",
              "rollupFieldId": "name",
              "fn": "lookup"
            }
          },
          "Email": {
            "id": "email",
            "type": "email",
            "constraint": {
              "required": true
            }
          },
          "Phone": {
            "id": "phone",
            "type": "string"
          },
          "Employment Status": {
            "id": "employment_status",
            "type": "select",
            "constraint": {
              "max": 1,
              "required": true
            },
            "option": {
              "options": [
                {
                  "id": "full_time",
                  "name": "Full-time",
                  "color": "green"
                },
                {
                  "id": "part_time",
                  "name": "Part-time",
                  "color": "yellow"
                },
                {
                  "id": "contract",
                  "name": "Contract",
                  "color": "orange"
                },
                {
                  "id": "intern",
                  "name": "Intern",
                  "color": "blue"
                }
              ]
            }
          }
        },
        "views": {
          "All Employees": {
            "type": "grid",
            "sort": [
              {
                "fieldId": "name",
                "direction": "asc"
              }
            ]
          },
          "By Position": {
            "type": "kanban",
            "kanban": {
              "field": "position"
            }
          }
        },
