          },
          "Employee Name": {
            "id": "employee_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "employee",
              "rollupFieldId": "name",
              "fn": "lookup"
            }
          },
          "Reviewer Name": {
            "id": "reviewer_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "reviewer",
              "rollupFieldId": "name",
              "fn": "lookup"
            }
          }
        },
        "views": {
