      },
      "Departments": {
        "fieldsOrder": [
          "Department Name",
          "Manager",
          "Manager Name",
          "Budget",
          "Location",
          "Employees",
          "Employees Count"
        ],
        "schema": {
          "Department Name": {
            "id": "department_name",
            "type": "string",
            "constraint": {
              "required": true
            },
            "display": true
          },
          "Manager": {
            "id": "manager",
            "type": "reference",
            "option": {
              "createSymmetricField": true,
              "foreignTable": {
                "tableName": "Employees"
              }
            }
          },
          "Manager Name": {
            "id": "manager_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "manager",
